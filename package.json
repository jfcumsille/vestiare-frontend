    "test:vitest": "vitest run",
    "build": "vue-tsc --project tsconfig.build.json --noEmit && vite build",
    "watch:test": "run-p watch:test:*",
    "watch:test:vitest": "vitest watch",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "^3.2.25"
  },
  "devDependencies": {
