    "humps": "^2.0.1",
    "i18next": "^21.6.11",
    "npm-run-all": "^4.1.5",
    "pinia": "^2.0.11",
    "postcss": "^8.4.6",
    "postcss-html": "^1.3.0",
    "postcss-scss": "^4.0.3",
    "rut-helpers": "^0.1.1",
    "sass": "^1.49.7",
    "stylelint": "^14.5.1",
    "stylelint-config-recommended": "^7.0.0",
    "stylelint-config-standard": "^25.0.0",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.5.4",
    "vite": "^2.8.0",
    "vitest": "^0.7.7",
    "vue-router": "^4.0.12",
    "vue-tsc": "^0.29.8"
  },
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=16.0.0"
