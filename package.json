{
  "name": "fintoc-front",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint --ext .js,.ts,.vue .",
    "lint:stylelint": "stylelint **/*.{css,scss,vue}",
